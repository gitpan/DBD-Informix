echo "***** Removing old dbdimp.c *****"
rm -f dbdimp.c
echo "***** Generating new Makefile *****"
perl Makefile.PL
echo "***** Attaching preprocessor information to Makefile *****"
echo ".SUFFIXES: .ec" >> Makefile
echo "" >> Makefile.
echo ".ec.o:" >> Makefile
echo "	esql -c -I$(ESQL_LIB) -I$(PERL_LIB) -I$(PERL_ARCHLIB) -I$(PERL_ARCHLIB)/DBI -I$(PERL_ARCHLIB)/CORE dbdimp.ec\
" >> Makefile
#echo "	cc -c -I$(ESQL_LIB) -i$(PERL_LIB) -I$(PERL_ARCHLIB) -I$(PERL_ARCHLIB)/DBI -I$(PERL_ARCHLIB)/CORE dbdimp.c" >> Makefile
echo "***** Generating MAXCURSOR information *****"
perl pec2ec 256 dbdimp.pec > dbdimp.ec
